        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a21" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE>Well</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a22" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE> I think, it is now my turn to have a word</ANNOTATION_VALUE>
